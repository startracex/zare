{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "insertPragma": false,
  "requirePragma": false,
  "proseWrap": "preserve",
  "experimentalSortPackageJson": false,
  "ignorePatterns": ["**/CHANGELOG.md", ".github/**/*.yml", "**/_pagefind"],
}
